{
  "$schema": "http://bigcontent.io/cms/schema/v1/schema#",
  "id": "{CONTENT_TYPE_BASEPATH}/product-list.json",
  "title": "Product list",
  "description": "Ulta Product List for Events Page",
  "allOf": [
    {
      "$ref": "http://bigcontent.io/cms/schema/v1/core#/definitions/content"
    }
  ],
  "type": "object",
  "properties": {
    "cards": {
      "type": "array",
      "items": {
        "allOf": [
          {
            "$ref": "http://bigcontent.io/cms/schema/v1/core#/definitions/content-link"
          },
          {
            "properties": {
              "contentType": {
                "enum": [
                  "{CONTENT_TYPE_BASEPATH}/simple-card.json",
                  "{CONTENT_TYPE_BASEPATH}/product-card.json"
                ]
              }
            }
          }
        ]
      },
      "minItems": 1,
      "maxItems": 30,
      "title": "Event Page Product & Marketing Cards"
    }
  },
  "propertyOrder": ["header", "cards"]
}