{
    "contentTypeUri": "{CONTENT_TYPE_BASEPATH}/rich-text.json",
    "settings":
    {
        "label": "Rich Text",
        "visualizations": [
        {
            "label": "Amplience CRS",
            "templatedUri":"https://{{vse.domain}}/v1/content/store/content-item/{{content.sys.id}}?template=acc-template-visualization&timestamp={{context.timestamp}}&locale={{locales}},en-US,*",
            "default": true
        },
        {
            "label": "JSON",
            "templatedUri":"https://{{vse.domain}}/content/id/{{content.sys.id}}?depth=all&format=inlined",
            "default": false
        }
        ],
        "icons": [
        {
            "size": 256,
            "url": "{ICON_BASEPATH}/rich-text.icon.png"
        }],
        "cards": [
        {
            "templatedUri":"https://{{vse.domain}}/v1/content/store/content-item/{{content.sys.id}}?template=acc-template-visualization&timestamp={{context.timestamp}}&locale={{locales}},en-US,*"
        }]
    },
    "repositories": {REPOSITORIES}
}