{
    "contentTypeUri": "{CONTENT_TYPE_BASEPATH}/algolia-personalised-content.json",
    "settings":
    {
        "label": "Algolia Dynamic Content",
        "visualizations": [
        {
            "label": "Amplience CRS",
            "templatedUri":"https://{{vse.domain}}/v1/content/store/content-item/{{content.sys.id}}?template=acc-template-visualization&timestamp={{context.timestamp}}&locale={{locales}},en-US,*",
            "default": true
        },
        {
            "label": "JSON",
            "templatedUri":"https://presalesadisws.s3.eu-west-1.amazonaws.com/dynamic-content/dc-visualiization-content-delivery/index.html?vse={{vse.domain}}&id={{content.sys.id}}&depth=all&format=inlined",
            "default": false,
            "cachebust": false
        }
        ],
        "icons": [
        {
            "size": 256,
            "url": "{ICON_BASEPATH}/algolia-personalised-content.icon.png"
        }],
        "cards": [
        {
            "templatedUri":"https://{{vse.domain}}/v1/content/store/content-item/{{content.sys.id}}?template=acc-template-visualization&timestamp={{context.timestamp}}&locale={{locales}},en-US,*"
        }]
    },
    "repositories": {REPOSITORIES}
}